{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Start Vue Server",
            "type": "shell",
            "command": "npm run serve",
            "options": {
                "cwd": "${workspaceFolder}/tracker_vue"
            },
            "isBackground": true,
            "problemMatcher": {
                "owner": "custom",
                "pattern": {
                    "regexp": "^$"
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "Starting development server",
                    "endsPattern": "App running at"
                }
            }
        }
    ]
}